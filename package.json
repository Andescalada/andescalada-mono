{
  "name": "andescalada",
  "private": true,
  "version": "0.0.1",
  "author": "elevyg",
  "license": "MIT",
  "scripts": {
    "db-up": "pscale connect andescalada develop --port 3309",
    "dev:nextjs": "turbo run dev --filter=@andescalada/nextjs",
    "dev:expo": "turbo run dev --filter=expo-app",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "dev": "turbo run dev",
    "clean": "find . -name node_modules -o -name .next -o -name .expo -type d -prune | xargs rm -rf",
    "postinstall": "prisma generate",
    "lint-and-fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "prettier-format": "prettier --config .prettierrc '**/*.{json,js,jsx,ts,tsx,css,scss,md}' --write",
    "db-reset": "npx prisma migrate reset --skip-seed --skip-generate -f",
    "db-push": "npx prisma db push",
    "db-seed": "npx prisma db seed",
    "db-clean-seed": "yarn db-reset && yarn db-push && yarn db-seed"
  },
  "workspaces": {
    "packages": [
      "packages/*",
      "apps/*"
    ],
    "nohoist": [
      "**/@shopify/react-native-skia/**",
      "**/@shopify/react-native-skia",
      "**/react-native-reanimated",
      "**/react-native-reanimated/**"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "4",
    "@tanstack/react-query": "^4.3.9",
    "@trpc/client": "10.0.0-proxy-beta.18",
    "@trpc/next": "^10.0.0-proxy-beta.18",
    "@trpc/react": "^10.0.0-proxy-beta.18",
    "@trpc/server": "^10.0.0-proxy-beta.18",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@elevyg/prisma-zod-enums-generator": "0.0.14",
    "@types/react": "~18.0.17",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@upstash/redis": "^1.13.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4 || ^3 || ^2.3.0 || ^1.7.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.4.0",
    "prisma": "4",
    "prisma-enum-generator": "^1.0.3",
    "superjson": "^1.10.0",
    "turbo": "^1.3.1",
    "typescript": "^4.6.3"
  },
  "resolutions": {
    "@types/react": "17.0.43"
  }
}
