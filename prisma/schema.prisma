generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model User {
  id    String @id @default(uuid())
  name  String
  email String @unique
}

model Zone {
  id   String @id @default(uuid())
  name String
}
