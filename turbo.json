{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "build/**", "node_modules/.cache/metro/**", "dist/**"]
    },
    "update:preview": {},
    "update:production": {},
    "build:production": {},
    "dev": {
      "cache": false
    },
    "dev:offline": {
      "cache": false
    },
    "lint": {
      "outputs": []
    }
  }
}
