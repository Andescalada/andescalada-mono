{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "yarn env:development && APP_VARIANT=development yarn start",
    "prod": "yarn env:production && expo start --no-dev",
    "env:production": "cp .env.production .env",
    "env:development": "cp .env.development .env",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "build": "EAS_BUILD_AUTOCOMMIT=1 CI=1  eas build --profile production --non-interactive --platform all",
    "build:preview": "APP_VARIANT=preview CI=1 eas build --profile preview",
    "build:dev":"APP_VARIANT=development eas build --profile development --platform all",
    "build:dev-simulator":"APP_VARIANT=development eas build --profile development-simulator --platform ios",
    "update:preview": "yarn env:development && source .env && APP_VARIANT=preview eas update --branch preview  --platform all"
  },
  "dependencies": {
    "@andescalada/api": "*",
    "@andescalada/climbs-drawer": "*",
    "@andescalada/db": "*",
    "@andescalada/ui": "*",
    "@andescalada/utils": "*",
    "@cloudinary/url-gen": "^1.8.6",
    "@expo/react-native-action-sheet": "^3.13.0",
    "@gorhom/bottom-sheet": "^4",
    "@hookform/resolvers": "^2.9.8",
    "@openspacelabs/react-native-zoomable-view": "^2.0.4",
    "@react-native-community/netinfo": "9.3.0",
    "@react-native-picker/picker": "2.4.2",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/stack": "^6.2.3",
    "@reduxjs/toolkit": "^1.8.5",
    "@sentry/react-native": "4.2.2",
    "@shopify/react-native-skia": "0.1.141",
    "@shopify/restyle": "^2.1.0",
    "add": "^2.0.6",
    "big-integer": "^1.6.51",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.2",
    "expo": "~46.0.9",
    "expo-application": "~4.2.2",
    "expo-auth-session": "~3.7.1",
    "expo-community-flipper": "^46.0.2",
    "expo-constants": "~13.2.4",
    "expo-crypto": "~11.0.0",
    "expo-dev-client": "~1.3.0",
    "expo-device": "~4.3.0",
    "expo-file-system": "~14.1.0",
    "expo-font": "~10.2.0",
    "expo-image-picker": "~13.3.1",
    "expo-notifications": "~0.16.1",
    "expo-random": "~12.3.0",
    "expo-splash-screen": "~0.16.2",
    "expo-status-bar": "~1.4.0",
    "expo-tracking-transparency": "~2.3.1",
    "expo-updates": "~0.14.6",
    "jwt-decode": "^3.1.2",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-hook-form": "^7.35.0",
    "react-native": "0.69.6",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-mmkv": "^2.4.3",
    "react-native-offline": "^6.0.0",
    "react-native-reanimated": "~2.9.1",
    "react-native-redash": "^18.1.0",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-svg": "12.3.0",
    "react-native-web": "~0.18.7",
    "react-redux": "^8.0.2",
    "sentry-expo": "~5.0.0",
