{
  "name": "expo-app",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "npx expo start --dev-client",
    "build": "expo export --output-dir ./build --platform all",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "expo start --web",
    "dev": "APP_VARIANT=development yarn start",
    "dev:ios": "APP_VARIANT=development yarn ios",
    "dev:android": "APP_VARIANT=development yarn android",
    "dev:clean": "AVOID_MEMOIZED_NAVIGATION=true yarn dev -c",
    "dev:offline": "OFFLINE_DEV=true yarn dev --offline",
    "dev:preview": "APP_VARIANT=preview yarn start",
    "debug": "yarn dev --force-manifest-type=classic",
    "debug:offline": "yarn debug --offline",
    "debug:preview": "yarn dev:preview --force-manifest-type=classic",
    "load-env": "export $(cat .env | egrep -v '#|^$' | xargs) && echo \"Loaded .env variables.\"",
    "prod": "npx expo start --dev-client --no-dev",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "build:production": "EAS_BUILD_AUTOCOMMIT=1 CI=1   npx eas-cli@latest build --profile production --non-interactive --platform all --auto-submit --no-wait",
    "build:production-simulator": "EAS_BUILD_AUTOCOMMIT=1 CI=1 npx node scripts/load-local-production-envs.mjs npx eas-cli@latest build --profile production-simulator --non-interactive --local",
    "build:production-local": "APP_VARIANT=production EAS_BUILD_AUTOCOMMIT=1 CI=1  npx node scripts/load-local-production-envs.mjs npx eas-cli@latest build --profile production-local --local --non-interactive",
    "build:preview": "APP_VARIANT=preview CI=1  npx eas-cli@latest build --profile preview",
    "build:preview-local:ios": "npx node scripts/load-local-envs.mjs yarn build:preview --platform ios --local --non-interactive",
    "build:preview-local:android": "npx node scripts/load-local-envs.mjs yarn build:preview --platform android --local --non-interactive",
    "build:preview-simulator": "APP_VARIANT=preview CI=1 npx eas-cli@latest build --profile preview-simulator",
    "build:preview-simulator-local:ios": "npx node scripts/load-local-envs.mjs yarn build:preview-simulator --platform ios --local --non-interactive",
    "build:dev": "APP_VARIANT=development npx eas-cli@latest build --profile development --platform all",
    "build:dev-simulator": "APP_VARIANT=development npx eas-cli@latest build --profile development-simulator",
    "build:dev-simulator-local:ios": "npx node scripts/load-local-envs.mjs yarn build:dev-simulator --platform ios --local --non-interactive",
    "build:dev-local:ios": "npx node scripts/load-local-envs.mjs yarn build:dev --platform ios --local --non-interactive",
    "build:dev-simulator-local:android": "npx node scripts/load-local-envs.mjs yarn build:dev-simulator --platform android --local --non-interactive",
    "build:dev-local:android": "npx node scripts/load-local-envs.mjs yarn build:dev --platform android --local --non-interactive",
    "update:preview": "APP_VARIANT=preview npx node scripts/eas-update.mjs npx eas-cli@latest update -p all --branch preview --auto",
    "update:production": "APP_VARIANT=production npx node scripts/eas-update.mjs npx eas-cli@latest update -p all  --branch production --auto"
  },
  "dependencies": {
    "@andescalada/api": "*",
