{
  "cli": {
    "version": ">= 3.14.0",
    
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "pnpm": "9.9.0",
      "node": "20.16.0",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "channel": "development"
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "channel": "development-simulator"
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_VARIANT": "preview",
        "FLIPPER_DISABLE": "1"
      }
    },
    "preview-simulator": {
      "extends": "preview",
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "APP_VARIANT": "production",
        "FLIPPER_DISABLE": "1"
      }
    },
    "production-simulator": {
      "autoIncrement": false,
      "extends": "production",
      "ios": {
        "simulator": true
      }
    },
    "production-local": {
      "autoIncrement": false,
      "distribution": "internal",
      "extends": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "elevy@andescalada.org",
        "ascAppId": "6443438367",
        "appleTeamId": "W7ZDU4P3XP"
      }
    }
  }
}
