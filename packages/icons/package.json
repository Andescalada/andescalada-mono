{
  "name": "@andescalada/icons",
  "version": "1.0.0",
  "description": "Svg Icons for Andescalada ",
  "private": true,
  "exports": {
    "./NativeIcons": {
      "types": "./dist/src/NativeIcons.d.ts",
      "default": "./dist/src/NativeIcons.js"
    },
    "./WebIcons": {
      "types": "./dist/src/WebIcons.d.ts",
      "default": "./dist/src/WebIcons.js"
    }
  },
  "scripts": {
    "transform": "sh transform.sh && sh transform-web.sh",
    "build": "tsc"
  },
  "dependencies": {
    "@andescalada/tsconfig": "*"
  },
  "devDependencies": {
    "@svgr/cli": "^6.2.1",
    "@svgr/core": "^6.2.1",
    "svgo": "^2.8.0",
    "typescript": "5.1.3"
  }
}
